{
	"extends": "airbnb-base",
	"env": {
		"jest": true
	},
	"rules": {
		"indent": [ "error", "tab" ],
		"no-console": 0,
		"no-tabs": 0,
		"key-spacing": [2, {
			"singleLine": {
				"beforeColon": false,
				"afterColon": true
			},
			"multiLine": {
				"beforeColon": true,
				"afterColon": true,
				"align": "colon"
			}
		}]
	}
}
